{
  "name": "champions-league-tt",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./public",
    "copy:assets": "node ./scripts/copy-assets.js",
    "generate:sitemap": "node ./scripts/generate-sitemap.js",
    "generate:pages": "node -e 'require(\"./scripts/generate-pages.js\")()'",
    "watch:pages": "npm run generate:pages && node ./scripts/watch-pages.js",
    "build:styles": "sass ./client/styles/index.scss ./public/styles/index.css",
    "watch:styles": "sass --watch ./client/styles/index.scss ./public/styles/index.css",
    "build": "NODE_ENV=production webpack && npm run generate:pages && npm run build:styles && npm run copy:assets && npm run generate:sitemap",
    "dev": "NODE_ENV=development npm run copy:assets && webpack --watch & npm run watch:pages & npm run watch:styles & http-server -p 8000"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.3",
    "core-js": "^2.6.9",
    "fs-extra": "^8.0.1",
    "jsonfile": "^5.0.0",
    "universal-gallery": "^1.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.1",
    "ejs": "^2.6.2",
    "ejs-loader": "^0.3.3",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "netlify-lambda": "^1.4.13",
    "node-sass": "^4.12.0",
    "node-watch": "^0.6.3",
    "nunjucks": "^3.2.0",
    "sass": "^1.21.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  }
}
